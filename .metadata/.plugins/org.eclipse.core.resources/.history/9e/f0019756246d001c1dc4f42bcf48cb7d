package javaIOPackage;
import java.io.*;
public class FileInput {

	public static void main(String[] args) {
		try {
			FileInputStream fin = new FileInputStream("/media/ashish/New Volume/Hello.txt");
			int i = fin.read();
			System.out.println((char)i);
			fin.close();
		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
