package Multithreading;

public class ThreadClass {

	class A extends Thread{
		public void run() {
			int n = 5;
			for(int i=0;i<n;i++) {
				System.out.println(i);
			}
			System.out.println("Exited loop");
		}
	}
	class B extends Thread{
		public void run() {
			int n = 5;
			for(int i=0;i<n;i++) {
				System.out.println(i);
			}
			System.out.println("Exited loop");
		}
	
		class C extends Thread{
			public void run() {
				int n = 5;
				for(int i=0;i<n;i++) {
					System.out.println(i);
				}
				System.out.println("Exited loop");
			}
		
	public static void main(String[] args) {
		

	}

}
